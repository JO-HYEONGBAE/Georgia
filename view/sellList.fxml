<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500" prefWidth="700" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.SellController">
   <children>
      <Label layoutX="305.0" layoutY="14.0" prefHeight="32.0" prefWidth="97.0" text="판매 목록" textAlignment="CENTER">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <TableView fx:id="sellTableView" layoutX="37.0" layoutY="65.0" onMouseClicked="#sellTableView" prefHeight="320.0" prefWidth="632.0">
        <columns>
          <TableColumn minWidth="0.0" prefWidth="0.0" text="번호" />
          <TableColumn prefWidth="194.0" text="품명" />
            <TableColumn prefWidth="138.0" text="가격" />
            <TableColumn prefWidth="66.0" text="수량" />
            <TableColumn prefWidth="160.0" text="판매시간" />
            <TableColumn prefWidth="73.0" text="회원번호" />
            <TableColumn minWidth="0.0" prefWidth="0.0" text="체크" />
        </columns>
      </TableView>
      <Button fx:id="btnClose" layoutX="381.0" layoutY="455.0" mnemonicParsing="false" onAction="#btnClose" prefHeight="32.0" prefWidth="78.0" text="닫기" />
      <Button fx:id="btnRefund" layoutX="227.0" layoutY="455.0" mnemonicParsing="false" onAction="#btnRefund" prefHeight="32.0" prefWidth="78.0" text="환불" />
      <Label fx:id="txtDate" layoutX="402.0" layoutY="412.0">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label fx:id="txtCash" layoutX="559.0" layoutY="411.0">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <Label layoutX="631.0" layoutY="412.0" text="원">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Button fx:id="btnSales" layoutX="54.0" layoutY="409.0" mnemonicParsing="false" onAction="#btnSales" text="월별 매출">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
